  "main": "index.js",
  "scripts": {
    "start": "sst start",
    "build": "sst build",
    "deploy": "sst deploy",
    "remove": "sst remove",
    "console": "sst console",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext .ts --max-warnings=0",
    "lint-and-fix": "eslint . --ext .ts --max-warnings=0 --fix",
    "prettier-format": "prettier --config .prettierrc '**/*.ts' '**/*.js' --write",
    "prettier-check": "prettier --config .prettierrc '**/*.ts' '**/*.js' --check",
    "test": "npm run test-unit",
    "test-unit": "TEST_MODE=unit jest -c jest.config.js",
    "create-env": "STACK=MyStack zx createEnv.mjs",
    "test-int": "TEST_MODE=int jest -c jest.config.js",
    "test-e2e": "TEST_MODE=e2e jest -c jest.config.js",
    "test-all": "npm run test-unit && npm run test-int && npm run test-e2e",
    "db-migrate": "sequelize-cli db:migrate",
    "db-migrate-status": "sequelize db:migrate:status",
    "db-migrate-undo": "sequelize-cli db:migrate:undo",
    "db-migrate-undo-all": "npx sequelize-cli db:migrate:undo:all "
  },
